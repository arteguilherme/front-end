<section class="commerce-product-tabs py-5">
  <div class="container">
    <div class="row">
      <div class="col-md-6"><img class="w-75" src="./assets/images/tvsmart.png" alt=""></div>
      <div class="col-md-6">
        <div class="section-product-carousel">
          <header class="nav-product">
            <div class="d-flex justify-content-between">
              <h2>Televisores</h2>
              <div class="custom-nav owl-nav"></div>
            </div>
          </header>
          <div id="commerce-product-tabs-list" class="commerce-product-tabs-list owl-carousel owl-theme">
            <% for (product of products){%>
              <div class="item">
                <h2><%= product.name %></h2>
              </div>
              <% } %>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>