<section class="slide">
  <div class="swiper-container">
    <div class="swiper-wrapper">
      <div class="swiper-slide"
        style="background-repeat: no-repeat; background-size: cover; background-position: center center; width: 100%; height: 100%; opacity: 1; visibility: inherit; z-index: 20;">
        <div class="container">
          <div class="row justify-content-center">
            <div class="col-md-6 col-sm-6">
              <div class="mt-4">
                <h2 class="font-weight-bold slide-title"
                  style="-webkit-animation: text-focus-in 1s cubic-bezier(0.550, 0.085, 0.680, 0.530) both; animation: text-focus-in 1s cubic-bezier(0.550, 0.085, 0.680, 0.530) both;">
                  Samsung Galaxy A70</h2>
                <h3 class="font-weight-light text-xl-left mt-3 mb-5"
                  style="-webkit-animation: tracking-in-expand-fwd-bottom 0.8s cubic-bezier(0.215, 0.610, 0.355, 1.000) both; animation: tracking-in-expand-fwd-bottom 0.8s cubic-bezier(0.215, 0.610, 0.355, 1.000) both;">
                  Lorem ipsum dolor sit amet consectetur adipisicing elit.</h3>
                <button type="button" class="btn btn-warning mr-2 text-uppercase btn-sm"
                  style="-webkit-animation: scale-in-center 0.5s cubic-bezier(0.250, 0.460, 0.450, 0.940) 0.5s both; animation: scale-in-center 0.5s cubic-bezier(0.250, 0.460, 0.450, 0.940) 0.5s both;">Comprar
                  agora</button>
              </div>
            </div>
            <div class="col-md-6 col-lg-4">
              <img class="w-100" src="./assets/images/cellphone.png" alt=""
                style="-webkit-animation: slide-in-bottom 0.5s cubic-bezier(0.250, 0.460, 0.450, 0.940) both; animation: slide-in-bottom 0.5s cubic-bezier(0.250, 0.460, 0.450, 0.940) both;" />
            </div>
          </div>
        </div>
      </div>
      <div class="swiper-slide">Slide 2</div>
      <div class="swiper-slide">Slide 3</div>
    </div>
    <div class="swiper-pagination"></div>
  </div>
</section>
